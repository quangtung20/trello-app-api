{
  "name": "trello-app-api",
  "version": "1.0.0",
  "description": "API for trello clone app",
  "main": "src/server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-babel": "babel ./src -d ./build/src",
    "remove-build": "rmdir /s /q build",
    "create-build": "mkdir build",
    "clean": "npm run remove-build && npm run create-build",
    "build": "npm run clean && npm run build-babel",
    "production": "npm run build && node ./build/src/server.js",
    "dev": "nodemon --exec babel-node ./src/server.js",
    "start": "node ./build/src/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quangtung20/trello-app-api.git"
  },
  "author": "quangtung",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/quangtung20/trello-app-api/issues"
  },
  "homepage": "https://github.com/quangtung20/trello-app-api#readme",
  "dependencies": {
    "@babel/eslint-parser": "^7.15.8",
    "@babel/runtime": "^7.15.4",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "joi": "^17.4.2",
    "lodash": "^4.17.21",
    "mongodb": "^3.7.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
    "@babel/node": "^7.15.8",
    "@babel/plugin-transform-runtime": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "babel-plugin-module-resolver": "^4.1.0",
    "nodemon": "^2.0.13"
  }
}
